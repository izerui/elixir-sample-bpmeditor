<s:Skin xmlns="http://ns.adobe.com/mxml/2009"
        xmlns:s="library://ns.adobe.com/flex/spark"
 >

  <Metadata>
    [HostComponent("bpm.graphic.icons.BPMNIcon")]
  </Metadata>

  <s:Group>
    <s:Path data="M 0.00 0.00 L 12.60 0.00 L 12.60 16.20 L 0.00 16.20 Z">
      <s:stroke><s:SolidColorStroke weight="1" color="{hostComponent.closedStrokeColor}" caps="none"/></s:stroke>
      <s:fill><s:SolidColor color="{hostComponent.fillColor}" alpha="{hostComponent.filled ? 1 : 0}"/></s:fill>
    </s:Path>
    <s:Path data="M 1.80 2.70 L 10.80 2.70">
      <s:stroke><s:SolidColorStroke weight="1" color="{hostComponent.openStrokeColor}" caps="none"/></s:stroke>
    </s:Path>
    <s:Path data="M 10.80 6.30 L 1.80 6.30">
      <s:stroke><s:SolidColorStroke weight="1" color="{hostComponent.openStrokeColor}" caps="none"/></s:stroke>
    </s:Path>
    <s:Path data="M 10.80 9.90 L 1.80 9.90">
      <s:stroke><s:SolidColorStroke weight="1" color="{hostComponent.openStrokeColor}" caps="none"/></s:stroke>
    </s:Path>
    <s:Path data="M 1.80 13.50 L 10.80 13.50">
      <s:stroke><s:SolidColorStroke weight="1" color="{hostComponent.openStrokeColor}" caps="none"/></s:stroke>
    </s:Path>
  </s:Group>

</s:Skin>
